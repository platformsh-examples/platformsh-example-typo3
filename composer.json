{
  "name": "ksjogo/typo3-platformsh-base",
  "license": "MIT",
  "type": "project",
  "description": "Platformsh base for TYPO3 CMS",
  "authors": [
    {
      "name": "Johannes Goslar",
      "email": "johannes.goslar@kronberger-spiele.de"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ksjogo/bootstrap_package.git"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "typo3/cms": "^8.6.0",
    "typo3/cms-about": "^8.6.0",
    "typo3/cms-belog": "^8.6.0",
    "typo3/cms-beuser": "^8.6.0",
    "typo3/cms-context-help": "^8.6.0",
    "typo3/cms-documentation": "^8.6.0",
    "typo3/cms-felogin": "^8.6.0",
    "typo3/cms-filelist": "^8.6.0",
    "typo3/cms-fluid-styled-content": "^8.6.0",
    "typo3/cms-func": "^8.6.0",
    "typo3/cms-impexp": "^8.6.0",
    "typo3/cms-info-pagetsconfig": "^8.6.0",
    "typo3/cms-lowlevel": "^8.6.0",
    "typo3/cms-reports": "^8.6.0",
    "typo3/cms-rsaauth": "^8.6.0",
    "typo3/cms-rte-ckeditor": "^8.6.0",
    "typo3/cms-setup": "^8.6.0",
    "typo3/cms-sys-note": "^8.6.0",
    "typo3/cms-t3editor": "^8.6.0",
    "typo3/cms-tstemplate": "^8.6.0",
    "typo3/cms-viewpage": "^8.6.0",
    "typo3/cms-wizard-crpages": "^8.6.0",
    "typo3/cms-wizard-sortpages": "^8.6.0",

    "typo3-ter/introduction": "^2.3",
    "helhum/typo3-console": "^4.2.0",

    "bk2k/bootstrap-package": "dev-platform as 7.0"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": false
    }
  }
}
